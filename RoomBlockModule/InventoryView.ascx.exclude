   <style>

 .menuTabs
        {
            position:relative;
            top:1px;
            left:10px;                                          
        }
        .tab 
        {           
            border:Solid 1px #2fa4e7;
            padding: 5px 5px 5px 5px;            
            border-style:outset;
            /*background-color:#2fa4e7;*/
            color :white; 
        }
      
        .selectedTab
        {
            border:Solid 1px black;
            border-bottom:Solid 1px white;
            padding:0px 10px;
            background-color:white;
            
        }
        .tabBody
        {
            border:Solid 1px black;
            padding:20px;
            background-color:white;
        }

     </style>
<style>

         .btnnew 
         {
         	border-top-color: #c0ced6;
